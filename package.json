{
  "name": "mpdctrl",
  "version": "1.0.0",
  "description": "MPD control module for Node.js",
  "main": "lib/index.js",
  "repository": "https://github.com/cotko/mpdctrl",
  "author": "Cotko <cotkocot@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "standard \"lib/**/*.js\"",
    "lintfix": "standard \"lib/**/*.js\" --fix",
    "todos": "grep -irn -B 1 -A 1 --colour todo lib/ || echo none found",
    "todo": "grep -irn --colour -E '#\\s+@method' lib/api/spec/ || none missing",
