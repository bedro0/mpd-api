    "doc": "jsdoc -c ./jsdoc.conf.js",
    "docf": "rm -r docs && yarn doc || yarn doc"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "file-type": "^10.11.0",
    "mpdjs": "https://github.com/cotko/mpd.js.git#master"
  },
  "devDependencies": {
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": []
  }
}
